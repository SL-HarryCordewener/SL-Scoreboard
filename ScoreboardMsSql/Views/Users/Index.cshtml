@using ScoreboardMsSql.Models.Scoreboard
@model System.Collections.Generic.IEnumerable<ScoreboardMsSql.Models.Scoreboard.ScoreboardUsers>

@{
    ViewBag.Title = "User Admin Index";
}

<h2>@ViewBag.Title</h2>
<hr/>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<br/>
<table class="admin">
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>
    @foreach (ScoreboardUsers item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                @Html.ActionLink("Details", "Details", new {id = item.Id}) |
                @Html.ActionLink("Delete", "Delete", new {id = item.Id})
            </td>
        </tr>
    }

</table>